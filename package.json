{
  "name": "oxa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot",
    "build:stats": "ng build --configuration production --aot --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "lint": "ng lint --max-warnings 999",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write",
    "format:all": "prettier --write ./src/{**,**/**}/*.{ts,html}",
    "analyze": "webpack-bundle-analyzer dist/oxa/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.3",
    "@angular/cdk": "^16.0.2",
    "@angular/common": "^16.0.3",
    "@angular/compiler": "^16.0.3",
    "@angular/core": "^16.0.3",
    "@angular/forms": "^16.0.3",
    "@angular/material": "^16.0.2",
    "@angular/platform-browser": "^16.0.3",
    "@angular/platform-browser-dynamic": "^16.0.3",
    "@angular/router": "^16.0.3",
    "@aws-amplify/ui-angular": "^3.2.18",
    "@ngneat/until-destroy": "^9.2.3",
    "@ngxs/storage-plugin": "^3.8.1",
    "@ngxs/store": "3.8.1",
    "@rx-angular/cdk": "^15.0.0",
    "@rx-angular/template": "^15.2.0",
    "aws-amplify": "^5.2.4",
    "rxjs": "~7.8.0",
    "rxjs-zone-less": "^1.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^15.0.0",
    "@angular-devkit/build-angular": "^16.0.3",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~16.0.3",
    "@angular/compiler-cli": "^16.0.3",
    "@rx-angular/eslint-plugin": "^1.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-rxjs": "^5.0.2",
    "eslint-plugin-rxjs-angular": "^2.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "jest": "^29.5.0",
    "jest-fail-on-console": "^3.1.1",
    "jest-preset-angular": "^13.0.1",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.6",
    "prettier-eslint": "^15.0.1",
    "typescript": "~4.9.4",
    "webpack-bundle-analyzer": "^4.8.0",
    "yargs": "^17.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|html)": [
      "yarn lint:fix",
      "yarn format",
      "git add"
    ]
  }
}
